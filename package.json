    "@faker-js/faker": "^7.6.0",
    "@shelex/cypress-allure-plugin": "^2.31.0",
    "cypress": "^12.17.3",
    "cypress-browser-permissions": "^1.1.0",
    "esbuild": "^0.15.6"
  },
